{
  "root": true,
  "env": {
    "browser": true,
    "jquery": true,
    "mocha": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "Velocity": true,
    "browser": true,
    "expect": true
  },
  "rules": {
    "no-bitwise": 2,
    "camelcase": 0,
    "curly": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "wrap-iife": 0,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "no-use-before-define": 2,
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 1,
    "no-irregular-whitespace": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 1,
    "strict": [2, "never"],
    "max-params": 0,
    "max-depth": [
      2,
      3
    ],
    "max-statements": 0,
    "complexity": [
      1,
      5
    ],
    "max-len": 0,
    "no-var": 2,
    "semi": [2, "always"],
    "no-cond-assign": 2,
    "no-debugger": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-unused-expressions": 1,
    "block-scoped-var": 1,
    "no-iterator": 2,
    "linebreak-style": 0,
    "comma-style": [
      2,
      "last"
    ],
    "comma-spacing": [2, { "before": false, "after": true }],
    "no-loop-func": 1,
    "no-multi-str": 1,
    "require-yield": 0,
    "valid-typeof": 1,
    "no-proto": 2,
    "no-script-url": 2,
    "no-shadow": 2,
    "dot-notation": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-invalid-this": 0,
    "no-console": 0,
    "vars-on-top": 2,
    "brace-style": [2, "1tbs"],
    "array-bracket-spacing": [2, "never"],
    "block-spacing": 2,
    "computed-property-spacing": [2, "never"],
    "func-call-spacing": [2, "never"],
    "arrow-spacing": 2,
    "space-in-parens": [2, "never"],
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": [2, "always"],
    "keyword-spacing": 2 ,
    "template-curly-spacing": 2,
    "prefer-template": 2,
    "no-useless-rename": 2,
    "require-jsdoc": ["error", {
      "require": {
          "FunctionDeclaration": true,
          "MethodDefinition": true,
          "ClassDeclaration": true
      }
    }]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaVersion": 6,
    "impliedStrict": true
  }
}
